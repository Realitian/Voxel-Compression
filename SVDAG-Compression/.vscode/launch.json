    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch target",
            "type": "cppdbg",
            "request": "launch",
            "program": "${command:cmake.launchTargetPath}",
            "args": ["/media/remi/Data/Other/3d-models/crytek-sponza/sponza.obj", "6", "0", "-l2"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
            "sourceFileMap": {
                "/build/glibc-OTsEL5": "/usr/src/glibc"
            }
        }
    ]
}